<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
	<id>resources</id>
	<formats>
		<format>tar.gz</format>
	</formats>
	<baseDirectory>${project.artifactId}/${project.version}</baseDirectory>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>src/main/resources-excluded/mustache</directory>
			<outputDirectory>mustache</outputDirectory>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>src/main/resources-excluded/swagger</directory>
			<outputDirectory>swagger</outputDirectory>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>src/main/resources-excluded/handlebars</directory>
			<outputDirectory>handlebars</outputDirectory>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>src/main/resources-excluded/swagger3</directory>
			<outputDirectory>swagger3</outputDirectory>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>target/swagger-codegen-generators/handlebars/Java</directory>
			<outputDirectory>handlebars/Java</outputDirectory>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>target/swagger-codegen-generators/mustache/JavaSpring</directory>
			<outputDirectory>mustache/JavaSpring</outputDirectory>
			<filtered>false</filtered>
		</fileSet>
	</fileSets>
</assembly>
